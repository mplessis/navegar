<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:ViewModel="clr-namespace:Navegar.WPF.Exemple.ViewModel"
                    xmlns:View="clr-namespace:Navegar.WPF.Exemple.View">
    
    <DataTemplate DataType="{x:Type ViewModel:FirstViewModel}">
        <View:FirstView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type ViewModel:SecondViewModel}">
        <View:SecondView />
    </DataTemplate>
</ResourceDictionary>